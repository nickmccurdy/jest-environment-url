    "jest-util": "^28.1.3",
    "jsdom": "^20.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/dom": "^8.17.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@tsconfig/node14": "^1.0.3",
    "@types/jest": "^28.1.6",
    "@types/testing-library__jest-dom": "^5.14.5",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "prettier-config-nick": "^1.0.3",
    "typescript": "^4.7.4"
  },
  "babel": {
    "presets": [
